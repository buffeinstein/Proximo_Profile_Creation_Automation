{
  "name": "proximo-demo",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite --config vite.config.mts",
    "dev:server": "tsx watch src/server.ts",
    "worker": "tsx src/worker.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "build": "tsc && vite build --config vite.config.mts"
  },
  "dependencies": {
    "better-sqlite3": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "openai": "^5.21.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.2.0",
    "jest": "^29.6.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
